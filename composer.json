{
    "name": "yedincisenol/api-startup",
    "description": "The Laravel Framework Api Template.",
    "keywords": ["framework", "laravel", "Api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "dingo/api": "1.0.*@dev",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "^1.0",
        "league/oauth2-server": "^6.0.0",
        "laravel/passport": "v3.0.*",
        "berkayk/onesignal-laravel": "^0.9.7",
        "anlutro/l4-settings": "^0.5.0",
        "barryvdh/laravel-cors": "^0.9.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
          "tests/TestCase1.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php -r \"touch('storage/oauth-private.key');\"",
            "php artisan key:generate",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
